<template>\n  <div class=\"post-item\" @click=\"favoritePost(post.id)\">\n    <div class=\"user-profile__post\">\n      <div class=\"post-item__user\">\n        @{{ username }}\n      </div>\n      <div class=\"post-item__content\">\n        {{ post.content }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'PostItem',\n  props: {\n    username: {\n      type: String,\n      required: true\n    },\n    post: {\n      type: Object,\n      required: true\n    }\n  },\n  methods:{\n    favoritePost(id){\n      this.$emit('favorite', id)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.post-item {\n  padding: 20px;\n  background-color: white;\n  border-radius: 5px;\n  border: 1px solid #DFE3E8;\n  box-sizing: border-box;\n  cursor: pointer;\n  transition: all .25s ease;\n  max-width: 700px;\n}\n\n.post-item:hover {\n  transform: scale(1.1, 1.1);\n}\n\n.post-item__user {\n  font-weight: bold;\n}\n\n\n</style>\n"]}]}